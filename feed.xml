---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    
    <title type="text" xml:lang="en">{{ site.root_desc }}</title>
    <link type="application/atom+xml" href="http://paulstamatiou.com/feed/" rel="self"/>
    <link type="text" href="http://roginfarrer.com" rel="alternate"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://roginfarrer.com</id>
    <author>
        <name>Rogin Farrer</name>
    </author>
    <rights>Copyright (c) 2014, Rogin Farrer; all rights reserved.</rights>
    
    {% for post in site.posts limit:10 %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://roginfarrer.com{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://roginfarrer.com{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}{% include rss_footer.html %}</content>
    </entry>
    {% endfor %}
</feed>